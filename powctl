start(){
  set -x
  launchctl load /Users/brandan/Library/LaunchAgents/cx.pow.powd.plist
}

stop(){
  set -x
  launchctl unload /Users/brandan/Library/LaunchAgents/cx.pow.powd.plist
}

case "$1" in
  stop)
    stop
    ;;
  start)
    start
    ;;
  restart)
    stop
    start
    ;;
  *)
    echo "Usage: $0 start|stop|restart"
    exit 1
    ;;
esac
